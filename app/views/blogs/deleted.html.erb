<p id="notice"><%= notice %></p>

<h1>ブログ　削除済み一覧</h1>

<table>
  <thead>
    <tr>
      <th>タイトル</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blogs.each do |blog| %>
      <tr>
        <% if blog.status == 4 %>
        <td><%= blog.title %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
