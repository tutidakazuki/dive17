<p id="notice"><%= notice %></p>

<h1>ブログ　下書き一覧</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @blogs.each do |blog| %>
      <tr>
        <% if blog.status == 1 %>
        <td><%= blog.title %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
